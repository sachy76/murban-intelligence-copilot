[run]
source = src/murban_copilot
branch = True
omit =
    */tests/*
    */__init__.py
    */interface/streamlit_app.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if TYPE_CHECKING:
    @abstractmethod
fail_under = 90
show_missing = True

[html]
directory = htmlcov
